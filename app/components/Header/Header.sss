$blue: #333

.bg
  background: $blue

.logo
  position: relative
  color: #fff
  text-decoration: none
  font-size: 30px
  line-height: 50px
  font-family: "Arial, Helvetica, sans-serif"
  display: inline-block
  & img
    vertical-align: -7px
    margin-right: 7px
    margin-left: 1px

.content
  width: 900px
  margin: 0 auto
