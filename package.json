{
  "name": "jmpv4",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && node run start:prod || npm run start:dev",
    "start:dev": "babel-node ./server/dev",
    "start:prod": "npm run build && node dist/server.bundle.js",
    "build:client": "babel-node ./node_modules/.bin/webpack --verbose --colors --display-error-details --optimize-minimize --optimize-occurence-order --config ./webpack/prod.config.js",
    "build:server": "babel-node ./node_modules/.bin/webpack --verbose --colors --display-error-details --config ./webpack/server.config.js",
    "build": "npm run build:client && npm run build:server"
  },
  "dependencies": {
    "extract-text-webpack-plugin": "^1.0.1",
    "if-env": "^1.0.0",
    "react": "^15.1.0",
    "react-css-modules": "^3.7.6",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.0.15",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.6.2",
    "postcss": "^5.0.21",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-modules": "^0.5.0",
    "precss": "^1.4.0",
    "react-hot-loader": "^3.0.0-beta.1",
    "style-loader": "^0.13.1",
    "sugarss": "^0.1.3",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "keywords": [],
